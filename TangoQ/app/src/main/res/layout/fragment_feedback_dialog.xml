            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="12dp"
            android:text="매우 고강도"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/rgIntensity"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView62"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="운동 피로도"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/rgIntensity" />

        <RadioGroup
            android:id="@+id/rgFatigue"
            style="@style/Widget.App.RadioButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView62">

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbFatigue1"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbFatigue2"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="48dp"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbFatigue3"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="48dp"
                android:layout_marginRight="48dp"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbFatigue4"
                style="@style/Widget.App.RadioButton"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="48dp"
                android:gravity="center"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbFatigue5"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />
        </RadioGroup>

        <TextView
            android:id="@+id/textView75"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:text="무통증"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/rgFatigue"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView76"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="미약한 통증"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/rgFatigue"
            app:layout_constraintStart_toEndOf="@+id/textView75" />

        <TextView
            android:id="@+id/textView77"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="보통"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/rgFatigue"
            app:layout_constraintEnd_toStartOf="@+id/textView78"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toEndOf="@+id/textView76" />

        <TextView
            android:id="@+id/textView78"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="12dp"
            android:text="상당한 통증"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/rgFatigue"
            app:layout_constraintEnd_toStartOf="@+id/textView79" />

        <TextView
            android:id="@+id/textView79"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="12dp"
            android:text="심각한 통증"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/rgFatigue"
            app:layout_constraintEnd_toEndOf="parent" />

        <RadioGroup
            android:id="@+id/rgSatisfaction"
            style="@style/Widget.App.RadioButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView63">

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbSatisfaction1"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbSatisfaction2"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="48dp"
                android:layout_weight="0"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbSatisfaction3"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="48dp"
                android:layout_marginRight="48dp"
                android:layout_weight="0"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbSatisfaction4"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="48dp"
                android:layout_weight="0"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />

            <com.google.android.material.radiobutton.MaterialRadioButton
                android:id="@+id/rbSatisfaction5"
                style="@style/Widget.App.RadioButton"
                android:layout_width="32dp"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:theme="@style/Widget.App.RadioButton"
                app:buttonTint="@color/tint_radio_btn" />
        </RadioGroup>

        <TextView
            android:id="@+id/textView63"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="운동 만족도"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
