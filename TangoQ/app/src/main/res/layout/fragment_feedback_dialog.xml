                            app:layout_constraintStart_toEndOf="@+id/tvFAccuracy"
                            app:layout_constraintTop_toTopOf="@+id/tvFAccuracy" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>
            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <TextView
